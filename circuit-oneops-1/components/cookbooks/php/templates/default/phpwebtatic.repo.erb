[phpwebtatic]
name=phpwebtatic
<% if node[:platform_version].start_with?("5") %>
baseurl=<%= @php_source %>/5/
<% elsif node[:platform_version].start_with?("6") %>
baseurl=<%= @php_source %>/6/
<% elsif node[:platform_version].start_with?("7") %>
baseurl=<%= @php_source %>/7/
<% else %>
baseurl=<%= @php_source %>/6/
<% end %>
enabled=1
gpgcheck=0
sslverify=0
